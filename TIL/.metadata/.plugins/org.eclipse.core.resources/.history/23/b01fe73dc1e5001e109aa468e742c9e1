package com.shinhan.day06.lab;


public class Parent {
	public String nation;
	
	public Parent() {
		this("대한민국");
		System.out.println("Parent() call");
	}
	public class Child extends Parent {
		public int studentNo;
		
		public Child(String name, int studentNo) {
			this.name = name;
			this.studentNo = studentNo;
		}
}


}