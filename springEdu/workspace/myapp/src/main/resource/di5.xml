<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id = "c1" class="com.shinhan.myapp.section05.ComputerVO">
		<property name = "model" value="ABC"></property>
		<property name = "price" value="100"></property>
		<property name = "os" value="window"></property>
	</bean>
	<bean id = "c2" class="com.shinhan.myapp.section05.ComputerVO">
		<property name = "model" value="DEF"></property>
		<property name = "price" value="200"></property>
		<property name = "os" value="IOS"></property>
	</bean>
	
	<bean id="company" class="com.shinhan.myapp.section05.CompanyVO" p:compName="신한DS" p:address="서울시 중구">
	<property name="computerList">
		<list>
			<ref bean="c1"></ref>
			<ref bean="c2"></ref>
		</list>
	</property>
	</bean>
	
</beans>
